      </span>
    </div>
    <div>
      <div id="levels_full" class="pull-left hidden-xs level-select">
        <a href="/my-games/beginner/1" id="beginner"
          class="level-select-link"><span>Beginner</span></a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <a href="/my-games/intermediate/1" id="intermediate"
          class="level-select-link"><span>Intermediate</span></a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <a href="/my-games/expert/1" id="expert"
          class="level-select-link"><span>Expert</span></a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <a href="javascript:void(0)" id="level_select_3"
          class="level-select-link"><span>Custom</span></a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      </div>
      <div class="clearfix"></div>
    </div>
  </div>
  <div class="page-block" id="S59" style="display: block;">
    <div id="stat_filters" class="stat-filters" style="width: 100%;">
      <div id="is_all_games_wrapper" class="pull-left">
        <label class="checkbox-fixed">
          <input id="is_all_games" type="checkbox" <%-isCheck%> ><span>All games</span>
        </label>
      </div>
      <div id="R28" class="pull-right"></div>
      <div class="clearfix"></div>
    </div>
    <table id="stat_table" class="table table-bordered table-stat" style="width: 100%; opacity: 1;">
      <thead id="stat_table_head">
        <tr>
          <th class="stat-rank-column">#</th>
          <th class="stat-time-column">Time</th>
          <th class="stat-state-column">Result</th>
          <th class="stat-3bv-extra-column">Clicks</th>
          <th class="" colspan="1">Date</th>
        </tr>
      </thead>
      <tbody id="stat_table_body"></tbody>
      <%-tableData%>
        </tbody>
    </table>
    <nav id="stat_pagination_nav">
      <ul id="stat_pagination" class="pagination pagination-sm noselect">
        <%-include('navigation.ejs');%>
      </ul>
    </nav>
  </div>
  <script>
    const url = window.location.pathname.split('/');
    url.pop();
    const mode = url.pop();
    document.getElementById(mode).className = `level-select-link active`;

    document.getElementById('is_all_games').onclick = function () {
      const params = window.location.pathname.split('/');
      params.pop();
      const mode = params.pop();
      $.ajax({ url: '/all', type: 'POST', data: { 'mode': mode, 'isCheck': this.checked } });
      window.location.href = `/my-games/${mode}/1`;
    };
  </script>
  <%- include('../layout_main_down.ejs',{mode:-1,cur_ws:0}); %>